<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="beancollection" class="com.wyett.springdemo.di.dicollection.BeanCollection">
        <property name="listCollection" >
            <list>
                <value>INDIA</value>
                <value>Pakistan</value>
                <value>USA</value>
                <value>USA</value>
            </list>
        </property>
        <property name="setCollection">
            <set>
                <value>INDIA</value>
                <value>Pakistan</value>
                <value>USA</value>
                <value>Japen</value>
            </set>
        </property>
        <property name="mapCollection">
            <map>
                <entry key="1" value="INDIA" />
                <entry key="2" value="Pakistan" />
                <entry key="3" value="USA" />
                <entry key="4" value="Japen" />
            </map>
        </property>
        <property name="propsCollection">
            <props>
                <prop key="one">INDIA</prop>
                <prop key="two">Pakistan</prop>
                <prop key="three">USA</prop>
                <prop key="four">Japen</prop>
            </props>
        </property>
    </bean>

</beans>